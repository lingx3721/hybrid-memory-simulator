machine:
  nvmain_test_shared_l3:
    _file: /home/ling/githubs/hybrid-memory-simulator/config/new-machine/moesi.conf
    caches:
    - insts: $NUMCORES
      name_prefix: L1_I_
      option: {private: true}
      type: l1_32K_moesi
    - insts: $NUMCORES
      name_prefix: L1_D_
      option: {private: true}
      type: l1_32K_moesi
    - insts: $NUMCORES
      name_prefix: L2_
      option: {last_private: true, private: true}
      type: l2_512K_moesi
    - insts: 1
      name_prefix: L3_
      option: {private: false}
      type: l3_2M
    cores:
    - {name_prefix: ooo_, type: ooo}
    description: public shared l3 cache with nvm memory
    interconnects:
    - connections:
      - {L1_I_$: UPPER, core_$: I}
      - {L1_D_$: UPPER, core_$: D}
      - {L1_I_$: LOWER, L2_$: UPPER}
      - {L1_D_$: LOWER, L2_$: UPPER2}
      - {L3_0: LOWER, MEM_0: UPPER}
      type: p2p
    - connections:
      - {DIR_0: DIRECTORY, L2_*: LOWER, L3_0: UPPER}
      type: switch
    memory:
    - {insts: 1, name_prefix: DIR_, type: global_dir_cont}
    - {insts: 1, name_prefix: MEM_, type: nvmain_cont}
    min_contexts: 2
